{
  "extends": "../tsconfig.custom.base.json",
  "compilerOptions": {
    "lib": [
      "es2023",
      "dom",
      "dom.iterable"
    ],
    "composite": true,
    "target": "ES2023",
    "allowImportingTsExtensions": true,
    "allowArbitraryExtensions": true,
    "declaration": true,
    "emitDeclarationOnly": true,
    "jsx": "react-jsx",
    "outDir": "../out",
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "svg.d.ts"
  ]
}